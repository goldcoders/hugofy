    "prod": "cross-env NODE_ENV=production node --require dotenv/config ./esbuild.js"
  },
  "dependencies": {
    "@types/node": "^15.12.2",
    "netlify-cms": "^2.10.138",
    "netlify-cms-app": "^2.15.18",
    "netlify-cms-proxy-server": "^1.3.18",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@snowpack/plugin-dotenv": "^2.1.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "esbuild": "^0.12.8",
    "npm-run-all": "^4.1.5",
    "snowpack": "^3.5.6"
  }
}
